{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "github>Cyb3r-Jak3/renovate-configs//recommend-extended.json"
  ],
  "customManagers": [
    {
      "customType": "regex",
      "managerFilePatterns": [
        "/^docker-bake.hcl$/"
      ],
      "matchStrings": [
        "datasource=(?<datasource>.*?) depName=(?<depName>letsencrypt/unbound_exporter)\n.*=\\s\"(?<currentValue>.*)\""
      ]
    }
  ]
}
